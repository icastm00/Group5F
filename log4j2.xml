<?xml version="1.0" enconding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %logger{36} - %msg%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="error">
			<AppenderRef ref="Consolose"/>
		</Root>
	</Loggers>
</Configuration>
